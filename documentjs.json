{
  "sites": {
    "gaiden_css": {
      "static": "./theme/static",
      "templates": "./theme/templates",
      "forceBuild": true,
      "glob": "src/scss/**/*.{css,scss,md}",
      "dest": "docs/gaiden-css",
      "parent": "gaiden_css"
    },
    "gh_pages": {
      "static": "theme/static",
      "templates": "theme/templates",
      "glob": "src/scss/**/*.{css,scss,md}",
      "dest": "./",
      "parent": "gaiden_css"
    }
  }
}
